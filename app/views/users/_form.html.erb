<%= form_for @user do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :bio %>
  <%= f.text_field :bio %>

  <%= f.label :admin %>
  <%= f.text_field :admin %>

  <%= f.label :location %>
  <%= f.text_field :location %>

  <%= f.label :image %>
  <%= f.url_field :image%>

  <%= f.submit %>
<% end %>
